                                        var atotal = time["atotal"].ToString();//是否有做题记录
                                        var iscw = time["iscw"].ToString();//是否有错误？ 
                                        var clas = "";//look_now_bt 回答错误  ,look_sure_bt 回答正确、look_no_bt 未回答
                                        if (siz != "0")
                                        {
                                            if (atotal != "0")
                                            {
                                                if (iscw != "0")
                                                {
                                                    clas = "color_now";
                                                }
                                                else
                                                {
                                                    clas = "color_look_sure";
                                                }
                                            }
                                            else
                                            {
                                                clas = "color_now";
                                            }
                                        }
                                        else
                                        {
                                            clas = "color_no";
                                        }
                                        
                                        if (i == 0)
                                        {
                                        }
                                        if ((i + 1) < 10)
