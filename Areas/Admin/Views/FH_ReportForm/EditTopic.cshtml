        border: none;
        border-bottom: none;
    }
    /*#tb_tbody label {
       font-weight:initial;
    }*/
    #tb_tbody .col-sm-22 {
        font-weight: initial;
    }

    #tb_tbody tr {
        text-align: right;
    }

        #tb_tbody tr td {
            padding-bottom: 0px;
        }

    #Add table {
        margin: 5px;
        width: 90%;
    }
    #Add .table-responsive {
       text-align:-webkit-center;
    }  
    #Add .table-responsive hr{
        width: 90%;
        height:2px;
        border:none;
        border-top:2px solid #555555;
    }  
    #Add .table-responsive .dw{
        width: 90%;
       text-align:right;
    }  
    #Add .table-responsive .dw label{
        margin-right:10px;
    }  

    #tb_tbody input:focus {
        outline: none;
        border: none;
        border-bottom: 1px solid #0288d1;
    }
    .td_bj {
        background-color: #c1dcd7;
    }
</style>
