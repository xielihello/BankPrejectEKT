<style>
     .tb_tbody input {
        width: 85%;
        border-left: 0px;
        border-top: 0px;
        border-right: 0px;
        /*border-bottom:0px*/
    }

    #tb_tbody input {
        width: 85%;
        text-align: center;
        border-left: 0px;
        border-top: 0px;
        border-right: 0px;
        /*border-bottom:0px*/
    }
     #tb_tbody input[readonly="true"] {
        width: 85%;
        text-align: right;
        border-left: 0px;
        border-top: 0px;
        border-right: 0px;
        border-bottom:0px
        

    } 
    #tb_tbody input[readonly="true"]:focus {
