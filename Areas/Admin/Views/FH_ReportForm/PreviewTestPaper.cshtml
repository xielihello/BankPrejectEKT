<script>
    var erial = 1;
    var len = 0;
    $(function () {
        var TitleName = "@ViewData["TitleName"]";
        var TestsFraction = "@TestsFraction";
        $("#TitleName").html(TitleName + "（满分 " + TestsFraction + "）");
        len = $("#timbt").find("button").length;
        if (len <= 0) {
            $("#fy").hide();
        }
        Pagediv(1);
    })
    function AjaxGetModel(obj) {
        // $("button").next(".btnss_outline").addClass("btn-outline")
        // debugger
        // $(obj).removeClass("btn-outline")
        //var TopicId = $(obj).attr("TopicId");
        //var Title = $(obj).attr("Title")
        //var Fraction = $(obj).attr("Fraction")
        //GetModelById(TopicId, Title, Fraction);
        var TopicId = 0;
        var Title = "";
        var Fraction = 0;

        $("#timbt").find("button").each(function (i) {
            if (obj == (i + 1)) {
                TopicId = $(this).attr("TopicId");
                Title = $(this).attr("Title")
                Fraction = $(this).attr("Fraction")
                $(this).removeClass("btn-outline")
            } else {
                $(this).addClass("btn-outline")
            }
        });
        GetModelById(TopicId, Title, Fraction);
    }
    function Pagediv(type) {
        var TopicId = 0;
        var Title = "";
        var Fraction = 0;

        if (type == 2) { //上一页
            if (erial > 1) {
                erial = erial - 1;
            }

        } else if (type == 3) { //下一页
            if (erial < len) {
                erial = erial + 1;
            }

        } else if (type == 4) {//尾一页
            erial = len;
        } else {
            erial = 1;
        }
        //$(this).parent().next().show();
        $("#timbt").find("button").each(function (i) {
            if (erial == (i + 1)) {
                TopicId = $(this).attr("TopicId");
                Title = $(this).attr("Title")
                Fraction = $(this).attr("Fraction")
                $(this).removeClass("btn-outline")
            } else {
                $(this).addClass("btn-outline")
            }
        });
        GetModelById(TopicId, Title, Fraction);
    }
</script>
