}
<div class="row  border-bottom white-bg dashboard-header">
    <h3>试卷预览 <a onclick="jsReturn()" class="btn btn-w-m  btn-white m-t-n-md  pull-right"><i class="fa fa-mail-reply"></i>返回</a></h3>

    <div class="ibox-content">
        <div class="row">
            <div class="text-center">
                <strong style="font-size: large" id="TitleName">@ViewData["TitleName"]（满分 @ViewData["TestsFraction"]）</strong>
            </div>
            <div class="hr-line-dashed "></div>
            <div class="row text-center" id="timbt">
                @for (int i = 0; i < tabl.Rows.Count; i++)
                {
                    var time = tabl.Rows[i];
                    string t = "第" + (i + 1) + "题";
                    int TopicId = Convert.ToInt32(time["TopicId"].ToString());
                    string Title = time["Title"].ToString();
                    var Fraction = !string.IsNullOrEmpty(time["TestsFraction"].ToString()) ? Convert.ToDecimal(time["TestsFraction"]) : 0;
                    TestsFraction += Fraction;
                    if (i == 0)
                    {
                        dycTopicId = TopicId;
                        dycTitle = Title;
                        dycFraction = Fraction;
                    }
                    if ((i + 1) == tabl.Rows.Count)
                    {
                        zhTopicId = TopicId;
                        zhTitle = Title;
                        zhFraction = Fraction;
                    }
                    <div class="col-sm-1 m-t-sm">
                        <button class="btn btn-primary btnss_outline btn-outline " topicid="@TopicId" title="@Title" fraction="@Fraction" onclick="AjaxGetModel(@(i + 1))">@t</button>
